@model IEnumerable<Bubblespace.Models.ApplicationUser>
@{
    Layout = "~/Views/Shared/_LayoutMain.cshtml";
}
<div class="container-fluid">
    <div class="row">
        <div id="list-view" class="col-xs-3">
              <ul>
                  @foreach (var user in Model) { 
                    <li class="list-item">@Html.ActionLink(user.NickName, "#", new { id = user.Id })</li>
                  }                  
              </ul>  
        </div>
        <div class="row">
            <div id="new-post" class="col-xs-8">
                <div>insert post here!</div>
            </div>
            <div id="main-view" class="col-xs-8">
                <ul>
                    <li class="content-item">This is a post</li>
                    
                </ul>  
            </div>
        </div>
    </div>
</div>